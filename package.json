{
    "name": "vscode-rpc",
    "displayName": "VSCode Rich Presence",
    "description": "Visual Studio Code Discord Rich Presence",
    "author": "Matheus Pedroni",
    "version": "1.0.2",
    "publisher": "mpedroni",
    "repository": {
		"type": "git",
		"url": "https://github.com/pedr0ni/vscode-rpc"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
		"discord",
		"vscode",
		"rich",
		"presence",
		"rich presence",
        "rpc",
        "vscode-rpc",
        "discord-rpc"
	],
    "activationEvents": [
        "onCommand:extension.activateRich",
        "onCommand:extension.disableRich"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.activateRich",
                "title": "Ativar Discord Rich Presence"
            },
            {
                "command": "extension.disableRich",
                "title": "Desativar Discord Rich Presence"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "discord-rpc": "^3.0.0-beta.12"
    }
}
